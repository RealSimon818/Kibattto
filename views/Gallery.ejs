<!-- Lightbox Modal -->
<div id="lightboxModal" class="modal fade" tabindex="-1" aria-labelledby="lightboxModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg modal-fullscreen-sm-down">
        <div class="modal-content">
            <div class="custom-drag-handle" style="width: 100%; height: 10px; cursor: move;"></div>
            <div class="modal-header">
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <button id="lightboxPrev" class="btn btn-secondary" style="position: absolute; left: 10px; top: 50%; transform: translateY(-50%); z-index: 1050;">&lt;</button>
                <img id="lightboxImage" src="" alt="" class="img-fluid d-none" style="width: 100%; height: 100%;">
                <video id="lightboxVideo" class="img-fluid d-none" style="width: 100%; height: 70vh;" controls></video>
                <button id="lightboxNext" class="btn btn-secondary" style="position: absolute; right: 10px; top: 50%; transform: translateY(-50%); z-index: 1050;">&gt;</button>
                <p id="lightboxCaption" class="text-center mt-2"></p>
            </div>
        </div>
    </div>
